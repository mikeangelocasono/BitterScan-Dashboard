{
  "name": "expert_dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache && echo 'Cleared build cache'",
    "clean:win": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && echo Cleared build cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && echo 'Cleared all build artifacts'",
    "clean:all:win": "if exist .next rmdir /s /q .next && if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && echo Cleared all build artifacts",
    "reinstall": "npm run clean:all && npm install",
    "reinstall:win": "npm run clean:all:win && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "@types/leaflet": "^1.9.21",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "framer-motion": "^11.18.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-is": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@swc/core": "^1.9.0",
    "@swc/helpers": "^0.5.17",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "^15.4.6",
    "lightningcss": "^1.27.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "lightningcss-darwin-arm64": "^1.27.0",
    "lightningcss-darwin-x64": "^1.27.0",
    "lightningcss-linux-arm64-gnu": "^1.27.0",
    "lightningcss-linux-arm64-musl": "^1.27.0",
    "lightningcss-linux-x64-gnu": "^1.27.0",
    "lightningcss-linux-x64-musl": "^1.27.0",
    "lightningcss-win32-x64-msvc": "^1.27.0"
  }
}
